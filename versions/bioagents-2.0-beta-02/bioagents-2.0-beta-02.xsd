												<xs:enumeration value="API documentation"/>
												<xs:enumeration value="Terms of use"/>
												<xs:enumeration value="Citation instructions"/>
												<xs:enumeration value="Manual"/>
												<xs:enumeration value="Training material"/>
												<xs:enumeration value="Other"/>
												<xs:enumeration value="General"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="comment" type="textType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Comment about the documentation.</xs:documentation>
											<xs:appinfo>
												<uiTip>Comment about the documentation.</uiTip>
												<bioagentsUsage>Optional</bioagentsUsage>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:all>
							</xs:restriction>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="publication" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>A publications about the software.</xs:documentation>
						<xs:appinfo>
							<uiTip>Publications about the software.</uiTip>
							<bioagentsUsage>Recommended</bioagentsUsage>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice>
								<xs:sequence>
									<xs:element name="doi" type="doiType">
