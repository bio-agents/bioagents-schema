											<xs:appinfo>
												<uiTip>Digital Object Identifier.</uiTip>
												<bioagentsUsage>Mandatory (conditionally)</bioagentsUsage>
											</xs:appinfo>
											<xs:documentation>Digital Object Identifier (DOI) of a publication about the software.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element ref="pmid" minOccurs="0"/>
									<xs:element ref="pmcid" minOccurs="0"/>
								</xs:sequence>
								<xs:sequence>
									<xs:element ref="pmid"/>
									<xs:element ref="pmcid" minOccurs="0"/>
								</xs:sequence>
								<xs:sequence>
									<xs:element ref="pmcid"/>
								</xs:sequence>
							</xs:choice>
							<xs:element name="type" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Type of publication. </xs:documentation>
									<xs:appinfo>
										<uiTip>Type of publication.</uiTip>
										<bioagentsUsage>Optional</bioagentsUsage>
									</xs:appinfo>
									<xs:documentation>For an explanation of valid values see github.com/bio-agents/bioagents-schema.</xs:documentation>
								</xs:annotation>
