					</xs:complexType>
				</xs:element>
				<xs:element name="contact" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Details of a contact for the software, e.g. developer or helpdesk.</xs:documentation>
						<xs:appinfo>
							<uiTip>Details of contacts for the software, e.g. developer or helpdesk.</uiTip>
							<bioagentsUsage>Mandatory</bioagentsUsage>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice>
								<xs:sequence>
									<xs:element name="email">
										<xs:annotation>
											<xs:documentation>Email address of contact.</xs:documentation>
											<xs:appinfo>
												<uiTip>Email address of contact.</uiTip>
												<bioagentsUsage>Mandatory (conditionally)</bioagentsUsage>
											</xs:appinfo>
											<xs:appinfo>
												<altova:exampleValues>
													<altova:example value="registry-support@iechor-dk.org"/>
												</altova:exampleValues>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:token">
												<xs:whiteSpace value="collapse"/>
												<xs:pattern value="[A-Za-z0-9_]+([-+.'][A-Za-z0-9_]+)*@[A-Za-z0-9_]+([-.][A-Za-z0-9_]+)*\.[A-Za-z0-9_]+([-.][A-Za-z0-9_]+)*"/>
												<xs:pattern value="[A-Za-z0-9_]+([-+.'][A-Za-z0-9_]+)*@[A-Za-z0-9_]+([-.][A-Za-z0-9_]+)*\.[A-Za-z0-9_]+([-.][A-Za-z0-9_]+)*"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="url" type="xs:anyURI" minOccurs="0">
										<xs:annotation>
											<xs:documentation>URL of contact.</xs:documentation>
											<xs:appinfo>
												<uiTip>URL of contact.</uiTip>
												<bioagentsUsage>Mandatory (conditionally)</bioagentsUsage>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:element name="url" type="xs:anyURI"/>
							</xs:choice>
							<xs:element name="name" type="nameType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Name of contact.</xs:documentation>
									<xs:appinfo>
										<uiTip>Name of contact.</uiTip>
										<bioagentsUsage>Optional</bioagentsUsage>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<xs:element name="tel" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Telephone number of contact. </xs:documentation>
									<xs:appinfo>
										<uiTip>Telephone number of contact. </uiTip>
										<bioagentsUsage>Optional</bioagentsUsage>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<xs:element name="type" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Type or role of contact.</xs:documentation>
									<xs:appinfo>
										<uiTip>Role of contact.</uiTip>
										<bioagentsUsage>Optional</bioagentsUsage>
									</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="enumType">
										<xs:enumeration value="Developer"/>
										<xs:enumeration value="Maintainer"/>
										<xs:enumeration value="Provider"/>
										<xs:enumeration value="Helpdesk"/>
										<xs:enumeration value="Mailing list"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="credit" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>An individual or organisation that should be credited for the software.</xs:documentation>
						<xs:appinfo>
							<uiTip>Individuals and organisations that should be credited for the software.</uiTip>
							<bioagentsUsage>Recommended</bioagentsUsage>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice>
								<xs:choice>
									<xs:element name="orcidId">
										<xs:annotation>
											<xs:appinfo>
												<uiTip>ORCID iD of person.</uiTip>
												<bioagentsUsage>Mandatory (conditionally)</bioagentsUsage>
											</xs:appinfo>
											<xs:documentation>Unique identifier (ORCID iD) of a person that is credited.</xs:documentation>
											<xs:documentation>Open Researcher and Contributor IDs (ORCID IDs) provide a persistent reference to information on a researcher, see http://orcid.org/. </xs:documentation>
											<xs:appinfo>
												<altova:exampleValues>
													<altova:example value="http://orcid.org/0000-0002-1825-0097"/>
												</altova:exampleValues>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:token">
												<xs:pattern value="http://orcid.org/[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{4}"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="gridId">
										<xs:annotation>
											<xs:appinfo>
												<uiTip>GRID ID of organisation.</uiTip>
												<bioagentsUsage>Mandatory (conditionally)</bioagentsUsage>
											</xs:appinfo>
											<xs:documentation>Unique identifier (GRID ID) of an organisation that is credited. </xs:documentation>
											<xs:documentation>Global Research Identifier Database (GRID) IDs provide a persistent reference to information on research organisations, see https://www.grid.ac/.</xs:documentation>
											<xs:appinfo>
												<altova:exampleValues>
													<altova:example value="grid.5170.3"/>
												</altova:exampleValues>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:token">
												<xs:pattern value="grid.[0-9]{4,}.[a-f0-9]{1,2}"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:choice>
								<xs:choice>
									<xs:sequence>
										<xs:element name="name">
											<xs:annotation>
												<xs:appinfo>
													<uiTip>Name of credited entity.</uiTip>
													<bioagentsUsage>Mandatory (conditionally)</bioagentsUsage>
												</xs:appinfo>
												<xs:documentation>Name of the entity that is credited.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="url" type="xs:anyURI" minOccurs="0">
											<xs:annotation>
												<xs:documentation>URL for the credited entity, e.g. homepage of an institute.</xs:documentation>
												<xs:appinfo>
													<uiTip>URL for credited entity.</uiTip>
													<bioagentsUsage>Mandatory (conditionally)</bioagentsUsage>
												</xs:appinfo>
												<xs:documentation>Not needed if an ID is specified.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:element name="url" type="xs:anyURI"/>
								</xs:choice>
							</xs:choice>
							<xs:element name="entity" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Type of entity that is credited.</xs:documentation>
									<xs:appinfo>
										<uiTip>Type of entity that is credited.</uiTip>
										<bioagentsUsage>Optional</bioagentsUsage>
									</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="enumType">
										<xs:enumeration value="Person"/>
										<xs:enumeration value="Project"/>
										<xs:enumeration value="Division"/>
										<xs:enumeration value="Institute"/>
										<xs:enumeration value="Consortium"/>
										<xs:enumeration value="Funding agency"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="role" minOccurs="0">
								<xs:annotation>
									<xs:appinfo>
										<uiTip>Role performed by credited entity.</uiTip>
										<bioagentsUsage>Optional</bioagentsUsage>
									</xs:appinfo>
									<xs:documentation>Role performed by credited entity.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="enumType">
										<xs:enumeration value="Developer"/>
										<xs:enumeration value="Maintainer"/>
										<xs:enumeration value="Provider"/>
										<xs:enumeration value="Contributor"/>
										<xs:enumeration value="Documentor"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="comment" type="textType" minOccurs="0">
								<xs:annotation>
									<xs:appinfo>
										<uiTip>A comment about the credit.</uiTip>
										<bioagentsUsage>Optional</bioagentsUsage>
									</xs:appinfo>
									<xs:documentation>A comment about the credit.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="dataType">
		<xs:annotation>
			<xs:documentation>Attributes of a agent parameter.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="data" type="EDAMdata">
				<xs:annotation>
					<xs:documentation>Type of data: EDAM Data term(s).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="format" type="EDAMformat" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Allowed format(s) of the data: EDAM Format term(s).</xs:documentation>
					<xs:appinfo>
						<uiTip>Allowed format(s) of the data: EDAM Format term(s).</uiTip>
						<bioagentsUsage>Recommended</bioagentsUsage>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="enumType">
		<xs:annotation>
			<xs:documentation>Enumeration of labels.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="nameType">
		<xs:annotation>
			<xs:documentation>Name of a thing.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="100"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="urlftpType">
		<xs:annotation>
			<xs:documentation>URL supporting FTP.</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="http://someURL.org"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:pattern value="https?://.+"/>
			<xs:pattern value="s?ftp://.+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="urlType">
		<xs:annotation>
			<xs:documentation>URL.</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="http://someURL.org"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:pattern value="https?://.+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="bioagentsIdType">
		<xs:annotation>
			<xs:documentation>URL in the bio.agents agent namespace (bio.agents/agent/ or bio.agents/t).</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="https://bio.agents/agent/agentName/version/versionNumber"/>
					<altova:example value="https://bio.agents/t/agentName/v/versionNumber"/>
					<altova:example value="https://bio.agents/agent/agentName"/>
					<altova:example value="https://bio.agents/t/agentName"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:pattern value="https?://bio.agents/(agent|t)/[A-Za-z0-9_\- _ ~]+((/(version|v)/)?[A-Za-z0-9_\- _ ~]+)?"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="bioagentsCollectionIdType">
		<xs:annotation>
			<xs:documentation>URL in the bio.agents collection namespace (bio.agents/collection or bio.agents/c).</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="https://bio.agents/collection/collectionName"/>
					<altova:example value="https://bio.agents/c/collectionName"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:pattern value="https?://bio.agents/(collection|c)/[A-Za-z0-9_\- _ ~]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ontologyConcept">
		<xs:annotation>
			<xs:documentation>A concept from an ontology.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="uri" type="xs:anyURI">
				<xs:annotation>
					<xs:documentation>Unique identifier of the concept (URI).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="term" type="xs:token" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An ontology concept term (preferred label or synonym).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="EDAMdata">
		<xs:annotation>
			<xs:documentation>EDAM Data concept.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:restriction base="ontologyConcept">
				<xs:all>
					<xs:element name="uri">
						<xs:annotation>
							<xs:documentation>URL of an EDAM Data concept.</xs:documentation>
							<xs:documentation>The URL must be in the EDAM Data namespace, i.e. http://edamontology.org/data_.</xs:documentation>
							<xs:documentation>
								<uiTip>EDAM Data URL.</uiTip>
								<bioagentsUsage>Mandatory (conditionally)</bioagentsUsage>
							</xs:documentation>
							<xs:appinfo>
								<altova:exampleValues>
									<altova:example value="http://edamontology.org/data_2976"/>
								</altova:exampleValues>
							</xs:appinfo>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:anyURI">
								<xs:pattern value="http://edamontology.org/data_[0-9]{4}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="term" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An EDAM  Data term (preferred label or synonym).</xs:documentation>
							<xs:appinfo>
								<uiTip>EDAM Data term (preferred label or synonym).</uiTip>
								<bioagentsUsage>Optional</bioagentsUsage>
							</xs:appinfo>
							<xs:appinfo>
								<altova:exampleValues>
									<altova:example value="Protein sequences"/>
								</altova:exampleValues>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EDAMformat">
		<xs:annotation>
			<xs:documentation>EDAM Format concept.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:restriction base="ontologyConcept">
				<xs:all>
					<xs:element name="uri">
						<xs:annotation>
							<xs:documentation>URL of an EDAM Forrmat concept.</xs:documentation>
							<xs:appinfo>
								<uiTip>EDAM Format URL.</uiTip>
								<bioagentsUsage>Mandatory (conditionally)</bioagentsUsage>
							</xs:appinfo>
							<xs:documentation>The URL must be in the EDAM  Format namespace, i.e. http://edamontology.org/format_.</xs:documentation>
							<xs:appinfo>
								<altova:exampleValues>
									<altova:example value="http://edamontology.org/format_1929"/>
								</altova:exampleValues>
							</xs:appinfo>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:anyURI">
								<xs:pattern value="http://edamontology.org/format_[0-9]{4}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="term" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An EDAM Format term.</xs:documentation>
							<xs:appinfo>
								<uiTip>EDAM Format term (preferred label or synonym)</uiTip>
								<bioagentsUsage>Optional</bioagentsUsage>
							</xs:appinfo>
							<xs:documentation>The term must be either the preferred label of the concept or a synonym of this term, as defined in EDAM.</xs:documentation>
							<xs:appinfo>
								<altova:exampleValues>
									<altova:example value="FASTA"/>
								</altova:exampleValues>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="doiType">
		<xs:annotation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="doi:10.1093/nar/gkv1116"/>
				</altova:exampleValues>
			</xs:appinfo>
			<xs:documentation>Digital Object Identifier.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="(doi:)?10.[0-9]{4,9}[A-Za-z0-9:;\)\(_/.-]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="textType">
		<xs:annotation>
