										<xs:element name="isNewVersionOf" type="bioagentsURL" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Unique ID of an agent in the IECHOR Agents and Data Services Registry which this agent is a new version of.  The ID must be a URL in the bio.agents namespace: see github.com/bio-agents/bioagentsxsd.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="isOldVersionOf" type="bioagentsURL" minOccurs="0">
