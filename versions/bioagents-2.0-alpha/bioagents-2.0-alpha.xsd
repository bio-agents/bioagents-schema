	<xs:complexType name="commandLineOption">
		<xs:annotation>
			<xs:documentation>Details of an option for a command line interface to a agent, i.e. something that can be specific by a flag such as '-a' or '--all'.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="optionSummary">
				<xs:annotation>
					<xs:documentation>A summary of the syntax for specifying a command-line option.  The value is markdown text adhering to a strict style and syntax, see https://github.com/bio-agents/bioagentsxsd.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="optionDescription">
				<xs:annotation>
					<xs:documentation>A textual description of a command-line option.  The value is markdown text adhering to a strict style and syntax, see https://github.com/bio-agents/bioagentsxsd.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="serviceEndpoint">
		<xs:annotation>
			<xs:documentation>Details of an endpoint to a service, i.e. a URL that can be used to invoke a particular function of a service.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="endpointURLTemplate">
				<xs:annotation>
					<xs:documentation>The template URL for a specific service endpoint, that defines the syntax of valid URL to call the service.  The value is markdown text adhering to a strict style and syntax, see https://github.com/bio-agents/bioagentsxsd.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endpointDescription">
				<xs:annotation>
					<xs:documentation>A textual description of a service endpoint.  The value is markdown text adhering to a strict style and syntax, see https://github.com/bio-agents/bioagentsxsd.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="output" type="Data"/>
		</xs:all>
	</xs:complexType>
	<xs:element name="resources">
		<xs:annotation>
			<xs:documentation>Description of one or more bioinformatics software resources, e.g. databases, agents and services, in the bioagentsXSD resource description format.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="resource" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Description of a bioinformatics software resource.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="resource">
		<xs:annotation>
			<xs:documentation>Attributes of bioinformatics resource</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="name">
					<xs:annotation>
						<xs:documentation>Canonical resource name assigned by the developer or maintainer of the resource.  Names used in the IECHOR Agents and Data Services Registry must be unique and conform to a style and syntax: see github.com/bio-agents/bioagentsxsd.</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="resourceName"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="Name">
							<xs:maxLength value="50"/>
							<xs:pattern value="[\p{Zs}A-Za-z0-9+\.,\-_:;()]*"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="homepage" type="URL">
					<xs:annotation>
						<xs:documentation>Homepage of the resource Web site, or some page that best serves this purpose (URL).</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="http://resourcename.org"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="description">
					<xs:annotation>
						<xs:documentation>Short textual description of the resource.  The description must conform to a style and syntax: see github.com/bio-agents/bioagentsxsd.</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="Description of resource goes here."/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="500"/>
							<xs:whiteSpace value="collapse"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="tags">
					<xs:annotation>
						<xs:documentation>Terms from controlled vocabularies describing scientific, technical and administrative details about the resource.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="topic" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>General scientific domain the resource serves or other general category for it: EDAM Topic term(s).</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:restriction base="ontologyConcept">
											<xs:all>
												<xs:element name="uri">
													<xs:annotation>
														<xs:documentation>URL of an EDAM Topic concept.</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="http://edamontology.org/topic_0003"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:anyURI">
															<xs:pattern value="http://edamontology.org/topic_[0-9]{4}"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="term" type="xs:token" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Preferred label of an EDAM Topic concept.</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:all>
										</xs:restriction>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="organismID" minOccurs="0">
								<xs:annotation>
									<xs:documentation>NCBI taxon ID of an organism that the resourcec is particularly relevant for.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:restriction base="ontologyConcept">
											<xs:all>
												<xs:element name="uri" type="xs:anyURI">
													<xs:annotation>
														<xs:documentation>NCBI taxonomy ID (URI).</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="term" type="xs:token" minOccurs="0">
													<xs:annotation>
														<xs:documentation>A term for the concept, usually the preferrred label as defined for that concept in the ontology.</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:all>
										</xs:restriction>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="GOterm" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:restriction base="ontologyConcept">
											<xs:all>
												<xs:element name="uri">
													<xs:annotation>
														<xs:documentation>Unique identifier of the concept (URI).</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:anyURI">
															<xs:pattern value="vcvc"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="term" minOccurs="0">
													<xs:annotation>
														<xs:documentation>A GO term</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:token">
															<xs:maxLength value="100"/>
															<xs:whiteSpace value="collapse"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:all>
										</xs:restriction>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="iechorStatus" minOccurs="0">
