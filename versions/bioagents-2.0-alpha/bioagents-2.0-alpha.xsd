				<xs:element name="resourceType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Basic resource type: "Database", "Agent", "Service", "Workflow", "Platform" or "Container".  See github.com/bio-agents/bioagentsxsd.</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="Agent"/>
